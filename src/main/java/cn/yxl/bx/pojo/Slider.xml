<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yxl.bx.dao.ISliderDAO">
	<!-- 查询所有的菜单 -->
	<select id="findAllSlider" resultType="cn.yxl.bx.pojo.Slider">
		select
		sliderid,title,slider_url as sliderUrl,status
		from bx_slider where
		status='0'
	</select>
</mapper>